<Window x:Class="WebLoadCompare.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:WebLoadCompare"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        Title="MainWindow"
        Width="600"
        Height="350"
        mc:Ignorable="d">
    <Grid>
        <WrapPanel>
            <Button x:Name="btnWebview"
                    Width="100"
                    Margin="5"
                    Click="btnWebview_Click"
                    Content="WebView2" />
            <Button x:Name="btnCefSharp"
                    Width="100"
                    Margin="5"
                    Click="btnCefSharp_Click"
                    Content="CefSharp" />
            <Button x:Name="btnMiniblink"
                    Width="100"
                    Margin="5"
                    Click="btnMiniblink_Click"
                    Content="Miniblink" />
            <Button x:Name="btnAwesomium"
                    Width="100"
                    Margin="5"
                    Click="btnAwesomium_Click"
                    Content="Awesomium" />
        </WrapPanel>
    </Grid>
</Window>
